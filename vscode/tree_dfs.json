{
	"graph":{
		"prefix": "graph",
		"body": [
			"class graph{",
			"public:",
			"    vector<vector<int>> g;",
			"    int n;",
			"    graph(int _n){",
			"        n=_n;",
			"        g.resize(n+1);",
			"    }",
			"    void readGraph(){",
			"        int a,b;",
			"        for(int i=1; i<n; i++){",
			"            cin>>a>>b;",
			"            g[a].pb(b);",
			"            g[b].pb(a);",
			"        }",
			"    }",
			"    void dfs(int nd, int par){",
			"        for(auto to:g[nd]){",
			"            if(to==par)continue;",
			"            dfs(to,nd);",
			"        }",
			"    }",
			"};",
		],
		"description": "basic implement of graph",
	}
}